{
  "portfoliodb": {
    "name": "portfoliodb",
    "host": "${PSQL_JDIAZS_HOST}",
    "port": "${PSQL_JDIAZS_PORT}",
    "user": "${PSQL_JDIAZS_USERNAME}",
    "password": "${PSQL_JDIAZS_PASSWORD}",
    "database": "${PSQL_JDIAZS_DB}",
    "connector": "postgresql",
    "ssl": false
  },
  "amazonS3": {
    "name": "amazonS3",
    "connector": "loopback-component-storage",
    "provider": "amazon",
    "key": "${AWS_SECRET_ACCESS_KEY_2}",
    "keyId": "${AWS_ACCESS_KEY_ID_2}"
  }
}
