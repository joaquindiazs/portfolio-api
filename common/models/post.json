{
  "name": "post",
  "plural": "posts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "post_id": {
      "type": "number",
      "id": true,
      "generated": true,
      "required": false
    },
    "url": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "string"
    },
    "category_id": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "category",
      "foreignKey": "category_id"
    },
    "tags": {
      "type": "hasMany",
      "model": "tag",
      "foreignKey": "post_id",
      "through": "post_tag"
    }
  },
  "acls": [],
  "methods": {}
}
